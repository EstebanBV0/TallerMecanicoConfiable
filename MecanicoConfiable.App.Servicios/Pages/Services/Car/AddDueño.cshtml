@page
@model AddDueñoModel
@{
}
<h2>Agregando Propietario  @Model.Vehiculo.Placa  </h2>

<a class="btn btn-primary" asp-page ="/Services/Car/ListVehiculo">Volver</a>

<form method="post">
    <input type = "hidden" asp-for ="Vehiculo.Placa"/>

   <select asp-for="Vehiculo.IdDueño" class="form-control">                
             @foreach (var Dueño in Model.Dueño)
    {                
                <option value="@Dueño.IdDueño">@Dueño.Nombre</option>            
    }
    </select>

    <!--<div class="form-group">
        <label asp-for ="Vehiculo.IdMecanico"></label>
        <input asp-for ="Vehiculo.IdMecanico" class="form-control">
    </div>-->

   <button type="submit" class="btn btn-primary">Guardar</button>
   
</form>