@page 
@model NivelesModel
@{
    ViewData["Title"] = "RevNiveles";
}



<form method="post" id = "adminAgregarmecanico" class="mt-5 mb-3 formuadmin"  >
 <h2>Registro de Niveles </h2>
 <a asp-page="/Services/Services" class="btn-defaoult">Volver</a>

    <div class="formularioc">
        <div class="mb-3">
            <label for="placa" class="form-label">Fecha</label>
            <input asp-for="Niveles.FechaHora"  class="form-control form-control-sm" id="placa"  />
        </div>
        
        <div class="mb-3">
            <label for="marca" class="form-label">Nivel Aceite</label>
            <input asp-for="Niveles.NivelAceite"  class="form-control form-control-sm" id="marca"  />
        </div>
      
        <div class="mb-3">
            <label for="modelo" class="form-label">Nivel Liquido Frenos</label>
            <input asp-for="Niveles.NivelLiquidoFrenos"  class="form-control form-control-sm" id="modelo"  />
        </div>        
        <div class="mb-3">
            <label for="pasajeros" class="form-label">NivelRefrigerante</label>
            <input asp-for="Niveles.NivelRefrigerante"  class="form-control form-control-sm" id="pasajeros"  />
        </div>
        <div class="mb-3">
            <label for="paisOrigen" class="form-label">Nivel Liquido Direccion</label>
            <input asp-for="Niveles.NivelLiquidDireccion"  class="form-control form-control-sm" id="paisOrigen"  />
        </div>
        <div class="mb-3">
            <label for="descripcion" class="form-label">Id Vehiculo</label>
            <input asp-for="Niveles.IdVehiculo"  class="form-control form-control-sm" id="descripcion"  />
        </div>
        
        <button type="submit" class = "btn btn-primary">Guardar</button>  
    </div>
</form>